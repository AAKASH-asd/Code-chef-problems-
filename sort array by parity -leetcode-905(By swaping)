class Solution {
    public int[] sortArrayByParity(int[] nums) {
      int x=0;
        int temp=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]%2==0){
                temp=nums[x];
                nums[x]=nums[i];
                nums[i]=temp;
                x++;
            }
        }
        return  nums;
        
        
        
        
